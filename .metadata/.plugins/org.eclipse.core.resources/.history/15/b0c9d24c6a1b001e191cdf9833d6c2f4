package com.uc.komedi.model.entities;

import jakarta.persistence.*;
import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

import java.io.Serializable;

@Entity
@Table(name = "imagens_realizacoes")
@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor
public class ImagemRealizacao implements Serializable {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    private String url;

    @ManyToOne
    @JoinColumn(name = "id_realizacao")
    private Realizacao realizacao;
}
